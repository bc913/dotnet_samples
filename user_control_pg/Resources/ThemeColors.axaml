<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Primary colors -->
    <Color x:Key="PrimaryLightColor">RoyalBlue</Color>
    <Color x:Key="PrimaryDarkColor">#ff416bdf</Color>
    
    <!--Common-->
    <Color x:Key="PrimaryVariantColor">#00418a</Color> <!--Blue 300-->
    <Color x:Key="SecondaryColor">#f4810c</Color> <!--Orange 800-->
