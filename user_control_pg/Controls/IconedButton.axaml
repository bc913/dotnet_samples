    <Style Selector="controls|IconedButton">
        <!--Set the defaults-->
        <Setter Property="IconBrush" Value="{DynamicResource AppPrimaryColor}"></Setter>
        <Setter Property="Template">
            <ControlTemplate>
                <Button Name="PART_IconButton"
                        Background="Transparent"
                        BorderThickness="0"
                        Command="{TemplateBinding Command}"
                        CommandParameter="{TemplateBinding CommandParameter}"
                        Width="{TemplateBinding ButtonWidth}"
                        Height="{TemplateBinding ButtonHeight}"
                        Padding="4">
                    
                    <ToolTip.Tip>
                        <StackPanel>
                            <TextBlock 
                                FontSize="12" 
                                Text="{TemplateBinding ToolTipText}"/>
                        </StackPanel>
                    </ToolTip.Tip>
                    
                    <Path Name="PART_IconPath"
                          HorizontalAlignment="Center"
                          VerticalAlignment="Center"
                          Width="{TemplateBinding IconSize}"
                          Height="{TemplateBinding IconSize}"
                          Data="{TemplateBinding IconData}"
                          Fill="{TemplateBinding IconBrush}"
                          Stretch="Uniform" />
                    
                    <!-- <Grid RowDefinitions="Auto"> -->
                    <!--     ~1~ Icon @1@ -->
                    <!--     <Viewbox -->
                    <!--              HorizontalAlignment="Center" -->
                    <!--              VerticalAlignment="Center" -->
                    <!--              Width="{TemplateBinding IconSize}" -->
                    <!--              Height="{TemplateBinding IconSize}" -->
                    <!--              Margin="4"> -->
                    <!-- -->
                    <!--         ~1~ <PathIcon Data="{TemplateBinding IconData}"></PathIcon> @1@ -->
                    <!--         <Path Name="PART_IconPath" -->
                    <!--               Data="{TemplateBinding IconData}" -->
                    <!--               Fill="{TemplateBinding IconBrush}" -->
                    <!--               Stretch="Uniform" /> -->
                    <!--     </Viewbox> -->
                    <!-- -->
                    <!--     ~1~ Text that appears on hover @1@ -->
                    <!--     ~1~ <TextBlock Grid.Row="1" @1@ -->
                    <!--     ~1~            Text="{TemplateBinding Text}" @1@ -->
                    <!--     ~1~            HorizontalAlignment="Center" @1@ -->
                    <!--     ~1~            TextWrapping="Wrap" @1@ -->
                    <!--     ~1~            TextAlignment="Center" @1@ -->
                    <!--     ~1~            Foreground="{TemplateBinding TextBrush}" @1@ -->
                    <!--     ~1~            FontSize="{TemplateBinding TextSize}" @1@ -->
                    <!--     ~1~            IsVisible="{Binding $parent[Button].IsPointerOver}"/> @1@ -->
                    <!-- </Grid> -->
                </Button>
                
            </ControlTemplate>
        </Setter>
    </Style>
    
    <Style Selector="controls|IconedButton /template/ Path#PART_IconPath">
        <Setter Property="StrokeThickness" Value="1" />
    </Style>
    
    <Style Selector="controls|IconedButton:pointerover /template/ Path#PART_IconPath">
        <Setter Property="StrokeThickness" Value="3" />
    </Style>
